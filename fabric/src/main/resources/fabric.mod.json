{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "DenisMasterHerobrine",
    "VeiTr"
  ],
  "contact": {
    "sources": "${mod_sources}",
    "issues": "${mod_issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "assets/the_afterdark/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.denismasterherobrine.afterdark.TheAfterdarkFabric"
    ],
    "fabric-datagen": [
      "dev.denismasterherobrine.afterdark.registry_fabric.DataGeneratorRegistry"
    ],
    "client": [
      "dev.denismasterherobrine.afterdark.client.TheAfterdarkFabricClient"
    ]
  },
  "mixins": [
    "the_afterdark.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=17",
    "fabric-api": "*"
  }
}